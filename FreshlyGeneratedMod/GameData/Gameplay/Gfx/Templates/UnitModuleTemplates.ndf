template TemplateUnitCriticalModule
[
   Module
] is TModuleSelector
(
    Default        = <Module>
    Condition      = ~/IfNotCadavreCondition
)

template TemplateUnitMissileCarriage
[
    Connoisseur
] is TModuleSelector
(
    Default        = TMissileCarriageModuleDescriptor
    (
        Connoisseur    = <Connoisseur>
    )
    Condition      = ~/IfNotCadavreCondition
)
